{
    "name": "mikailfaruqali/log-viewer",
    "description": "A modern, elegant, and feature-rich log viewer for Laravel applications. Experience a beautiful glassmorphism-inspired interface that makes viewing and managing your application logs both intuitive and visually appealing.",
    "keywords": [
        "laravel",
        "log",
        "viewer",
        "logging",
        "debug"
    ],
    "homepage": "https://github.com/mikailfaruqali/log-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikail Faruq Ali",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22|^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}